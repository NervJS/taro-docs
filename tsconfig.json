{
  "extends": "@tsconfig/docusaurus/tsconfig.json",
  "compilerOptions": {
    "lib": ["DOM", "ESNext"],
    "baseUrl": ".",
    "paths": {
      "@site/static/*": ["./static/*"]
    },
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "types": ["@types/node"]
  },
  "include": ["blog", "docs", "i18n", "packages", "scripts", "src", "typings"],
  "exclude": ["src/sw.js"]
}
