{
  "extends": "@tsconfig/docusaurus/tsconfig.json",
  "compilerOptions": {
    "lib": ["DOM", "ESNext"],
    "baseUrl": ".",
    "esModuleInterop": true,
    "paths": {
      "@site/static/*": ["./static/*"]
    },
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "types": ["node"]
  },
  "include": ["blog", "docs", "i18n", "packages", "scripts", "src", "versioned_docs", "typings"],
  "exclude": ["src/sw.js"]
}
